<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<window xmlns="http://schemas.haulmont.com/cuba/window.xsd"
        caption="msg://editorCaption"
        class="com.application.innova.web.listedcompany.ListedCompanyEdit"
        datasource="listedCompanyDs"
        focusComponent="fieldGroup"
        messagesPack="com.application.innova.web.listedcompany">
    <dsContext>
        <datasource id="listedCompanyDs"
                    class="com.application.innova.entity.ListedCompany"
                    view="_local"/>
    </dsContext>
    <dialogMode height="600"
                width="800"/>
    <layout expand="windowActions"
            spacing="true">
        <fieldGroup id="fieldGroup"
                    datasource="listedCompanyDs">
            <column width="250px">
                <field property="name"/>
                <field property="symbol"/>
            </column>
        </fieldGroup>
        <frame id="windowActions"
               screen="editWindowActions"/>
    </layout>
</window>
